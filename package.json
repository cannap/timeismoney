{
	"name": "timeismoney",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"watch": "paraglide-js compile --project ./project.inlang --watch",
		"test:unit": "vitest",
		"generate": "drizzle-kit generate:sqlite --out ./drizzle/migrations --breakpoints --schema=./src/lib/server/db/schema.ts",
		"migrate": "tsx drizzle/migrate",
		"postinstall": "paraglide-js compile --project ./project.inlang"
	},
	"devDependencies": {
		"@inlang/paraglide-js": "1.1.1",
		"@inlang/paraglide-js-adapter-vite": "^1.2.1",
		"@playwright/test": "^1.40.1",
		"@sveltejs/adapter-auto": "^3.1.0",
		"@sveltejs/kit": "^2.3.2",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@types/eslint": "8.56.0",
		"@typescript-eslint/eslint-plugin": "^6.18.1",
		"@typescript-eslint/parser": "^6.18.1",
		"@vite-pwa/sveltekit": "^0.3.0",
		"@vitejs/plugin-basic-ssl": "^1.1.0",
		"dotenv": "^16.3.1",
		"drizzle-kit": "^0.20.13",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.3",
		"tslib": "^2.6.2",
		"tsx": "^4.7.0",
		"typescript": "^5.3.3",
		"vite": "^5.0.11",
		"vite-plugin-mkcert": "^1.17.3",
		"vitest": "^1.1.3",
		"workbox-window": "^7.0.0"
	},
	"type": "module",
	"dependencies": {
		"@libsql/client": "0.4.0-pre.7",
		"@lucia-auth/adapter-drizzle": "1.0.0-beta.5",
		"drizzle-orm": "^0.29.3",
		"drizzle-valibot": "^0.1.1",
		"lucia": "3.0.0-beta.14",
		"oslo": "^0.27.1",
		"valibot": "^0.26.0"
	}
}
