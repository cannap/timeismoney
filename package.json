{
  "name": "timeismoney",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "watch": "paraglide-js compile --project ./project.inlang --watch",
    "test:unit": "vitest",
    "push": "drizzle-kit push:sqlite",
    "drop": "drizzle-kit drop --out=drizzle/migrations ",
    "generate": "drizzle-kit generate:sqlite --out ./drizzle/migrations --breakpoints --schema=./src/lib/server/db/schema.ts",
    "studio": "drizzle-kit studio",
    "migrate": "tsx drizzle/migrate",
    "postinstall": "paraglide-js compile --project ./project.inlang",
    "email:dev": "email dev --dir src/lib/emails"
  },
  "devDependencies": {
    "@inlang/paraglide-js": "1.1.1",
    "@inlang/paraglide-js-adapter-vite": "^1.2.15",
    "@playwright/test": "^1.41.2",
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/kit": "^2.5.1",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@types/eslint": "8.56.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vite-pwa/sveltekit": "^0.3.0",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "autoprefixer": "^10.4.17",
    "dotenv": "^16.4.5",
    "drizzle-dbml-generator": "^0.6.1",
    "drizzle-kit": "^0.20.14",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.35",
    "postcss-load-config": "^5.0.3",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.1",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "sass": "^1.71.1",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.4",
    "sveltekit-superforms": "^2.6.1",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "unplugin-icons": "^0.18.5",
    "valibot": "^0.28.1",
    "vite": "^5.1.4",
    "vitest": "^1.3.1",
    "workbox-window": "^7.0.0"
  },
  "type": "module",
  "dependencies": {
    "@inlang/paraglide-js-adapter-sveltekit": "^0.5.0",
    "@libsql/client": "0.5.2",
    "@lucia-auth/adapter-drizzle": "^1.0.2",
    "@react-email/components": "0.0.15",
    "@react-email/tailwind": "0.0.14",
    "await-sleep": "^0.0.1",
    "bits-ui": "^0.18.2",
    "clsx": "^2.1.0",
    "drizzle-orm": "^0.29.4",
    "drizzle-valibot": "^0.1.1",
    "emailjs": "^4.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "formsnap": "^0.5.0",
    "lucia": "^3.0.1",
    "lucide-svelte": "^0.331.0",
    "mode-watcher": "^0.2.1",
    "oslo": "^1.1.2",
    "radix-icons-svelte": "^1.2.1",
    "react": "^18.2.0",
    "react-email": "2.1.0",
    "svelte-sonner": "^0.3.18",
    "tailwind-merge": "^2.2.1",
    "tailwind-variants": "^0.2.0",
    "vite-plugin-pwa": "^0.18.2"
  }
}